# !/bin/bash
#
#  Flags :
#
#     -q : Si esta presente el programa no tendra salida grafica
#
#  Descripcion :
#
#   
#  
#  
bool="False"

while getopts 'q' opt
	do
		case $opt in
		q)
			echo "Use el -d"
			bool="True"
			;;

		\?)
			echo "Invalid"
			exit 1
			;;
		:)
			echo "Exit"
			exit 1
		esac
	done

if [ $bool = "True" ]
	then
		java -jar ./jcommon-1.0.23/jcommon-1.0.23.jar
		java -jar ./jfreechart-1.0.19/jfreechart-1.0.19-demo.jar
		java Main "0" $1
	
else
		java Main "1" $1

fi

