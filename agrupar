# !/bin/bash
#
#  Flags :
#
#     -q : Si esta presente el programa no tendra salida grafica
#
#  Descripcion :
#
#   
#  
#  
bool="False"

while getopts 'q' opt
	do
		case $opt in
		q)
			echo "Use el -d"
			bool="True"
			;;

		\?)
			echo "Invalid"
			exit 1
			;;
		:)
			echo "Exit"
			exit 1
		esac
	done

if [ $bool = "True" ]
	then
		java -Xmx4g -cp .:jcommon-1.0.0.jar:jfreechart-1.0.1.jar Main "0" $1
	
else
		java -Xmx4g -cp .:jcommon-1.0.0.jar:jfreechart-1.0.1.jar Main "1" $1

fi

